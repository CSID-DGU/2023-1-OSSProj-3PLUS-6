       console.error(err);\n      });\n  };\n\n  return (\n    <div className=\"sign-in-container\">\n      {/* 로고 이미지 */}\n      <div className=\"text-center\">\n      <img src=\"/loginLogo.png\" alt=\"loginLogo\" className=\"loginLogo\" />\n      </div>\n      {/* 로그인 폼 */}\n      <div className=\"form-container\">\n        <form onSubmit={handleSubmit} className=\"form\">\n          {/* 학번 입력 필드 */}\n          <Input\n            required\n            className=\"full-width-input\"\n            placeholder=\"학번\"\n            prefix={<UserOutlined />}\n            name=\"studentNum\"\n          />\n          {/* 비밀번호 입력 필드 */}\n          <Input.Password\n            required\n            className=\"full-width-input\"\n            placeholder=\"비밀번호\"\n            prefix={<KeyOutlined />}\n            name=\"password\"\n          />\n          {/* 로그인 버튼 */}\n          <Button type=\"primary\" htmlType=\"submit\" className=\"submit-button\">\n            LOGIN\n          </Button>\n          {/* 회원가입 링크 */}\n          <div className=\"signup-container\">\n              <p className=\"signup-text\">아직 계정이 없으신가요?</p>\n              <Link to=\"/signup\" className=\"signup-link\">\n                회원가입\n              </Link>\n          </div>\n        </form>\n      </div>\n    </div>\n  );\n};\n\nexport default SignIn;\n",{"ruleId":"37","severity":1,"message":"38","line":20,"column":10,"nodeType":"39","messageId":"40","endLine":20,"endColumn":19},{"ruleId":"37","severity":1,"message":"41","line":6,"column":10,"nodeType":"39","messageId":"40","endLine":6,"endColumn":20},{"ruleId":"37","severity":1,"message":"42","line":6,"column":30,"nodeType":"39","messageId":"40","endLine":6,"endColumn":34},{"ruleId":"37","severity":1,"message":"43","line":8,"column":26,"nodeType":"39","messageId":"40","endLine":8,"endColumn":31},{"ruleId":"37","severity":1,"message":"44","line":10,"column":7,"nodeType":"39","messageId":"40","endLine":10,"endColumn":13},{"ruleId":"37","severity":1,