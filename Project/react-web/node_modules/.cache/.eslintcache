\n              }}\n            >\n              <span\n                style={{\n                  width: \"48%\",\n                  height: \"500px\",\n                  background: \"#FAFAFA\",\n                }}\n              >\n                <h3\n                  sytle={{\n                    fontSize: \"15px\",\n                    padding: \"10px\",\n                  }}\n                >\n                  종합강의시간표목록\n                </h3>\n                <table\n                  style={{\n                    width: \"100%\",\n                    border: \"1px solid\",\n                  }}\n                >\n                  <tr>\n                    <th>교과목명</th>\n                    <th>교원명</th>\n                    <th>요일/시간</th>\n                    <th>강의실</th>\n                    <th>학수강좌번호</th>\n                    <th>수강정원</th>\n                    <th>신청인원</th>\n                  </tr>\n                  <tr>\n                    <td></td>\n                    <td></td>\n                    <td></td>\n                    <td></td>\n                    <td></td>\n                    <td></td>\n                    <td></td>\n                  </tr>\n                </table>\n              </span>\n              <Tabs\n                style={{\n                  width: \"48%\",\n                  height: \"500px\",\n                  background: \"#FAFAFA\",\n                }}\n                onChange={onChange}\n                type=\"card\"\n                const\n                items={new Array(2).fill(null).map((_, i) => {\n                  const id = String(i + 1);\n                  const views = [\"테이블뷰\", \"시간표뷰\"];\n                  return {\n                    label: views[i],\n                    key: id,\n                    children: `희망강의 수강신청 확인 뷰 ${id}`,\n                  };\n                })}\n              />\n              {/* <span\n                style={{\n                  width: \"48%\",\n                  height: \"300px\",\n                  background: \"#FAFAFA\",\n                }}\n              >\n                테이블뷰/시간표뷰\n              </span> */}\n            </div>\n          </Content>\n        </Layout>\n      </Layout>\n    </Layout>\n  );\n};\nexport default Application;\n",{"ruleId":"39","severity":1,"message":"40","line":9,"column":9,"nodeType":"41","messageId":"42","endLine":9,"endColumn":14},{"ruleId":"39","severity":1,"message":"43","line":2,"column":31,"nodeType":"41","messageId":"42","endLine":2,"endColumn":39},{"ruleId":"39","severity":1,"message":"44","line":2,"column":61,"nodeType":"41","messageId":"42","endLine":2,"endColumn":64},{"ruleId":"39","severity":1,"message":"45","line":2,"column":66,"nodeType":"41","messageId":"42","endLine":2,"endColumn":69},{"ruleId":"39","severity":1,"message":"46","line":8,"column":9,"nodeType":"41","messageId":"42","endLine":8,"endColumn":15},{"ruleId":"39","severity":1,"message":"40","line":9,"column":9,"nodeType":"41","messageId":"42","endLine":9,"endColumn":14},{"ruleId":"39","severity":1,"message":"47","line":2,"column":3,"nodeType":"41","messageId":"42","endLine":2,"endColumn":17},{"ruleId":"39","severity":1,"message":"48","line":3,"column":3,"nodeType":"41","messageId":"42","endLine":3,"endColumn":23},{"ruleId":"39","severity":1,"message":"49","line":4,"column":3,"nodeType":"41","messageId":"42","endLine":4,"endColumn":15},{"ruleId":"39","severity":1,"message":"50","line":8,"column":26,"nodeType":"41","messageId":"42","endLine":8,"endColumn":31},"no-unused-vars","'Title' is assigned a value but never used.","Identifier","unusedVar","'Checkbox' is defined but never used.","'Row' is defined but never used.","'Col' is defined but never used.","'Option' is assigned a value but never used.","'LaptopOutlined' is defined but never used.","'NotificationOutlined' is defined but never used.","'UserOutlined' is defined but never used.","'Sider' is assigned a value but never used."]